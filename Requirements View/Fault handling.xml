								<UML:ClassifierRole name="Fault Classification" xmi.id="EAID_5A5EB8A6_E620_44db_8447_5E6A13F3E47E" visibility="public" base="EAID_11111111_5487_4080_A7F4_41526CB0AA00">
									<UML:ModelElement.stereotype>
