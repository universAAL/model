								<UML:ClassifierRole name="License verification" xmi.id="EAID_4D6A00BF_43F9_40ea_B9AA_232F57C8DE82" visibility="public" base="EAID_11111111_5487_4080_A7F4_41526CB0AA00">
									<UML:ModelElement.stereotype>
