			<EAScenario name="Assisted Person skips the additional roles step" type="Alternate" weight="0,00" subject="EAID_C72B1A9C_ABAC_4fe8_8BAC_ABCEBD8F2A2E" xmi.id="EAID_CE2203F8_23B6_4f54_B8AC_389598F0180E"/>
			<EAScenario name="Assisted Person skips the group adding" type="Alternate" weight="0,00" subject="EAID_C72B1A9C_ABAC_4fe8_8BAC_ABCEBD8F2A2E" xmi.id="EAID_DF1F50CC_34EA_4824_AC66_2E99A6C15F1D"/>
			<EAScenario name="Basic Path" type="Basic Path" weight="5,00" subject="EAID_C72B1A9C_ABAC_4fe8_8BAC_ABCEBD8F2A2E" xmi.id="EAID_74D1DF44_421B_4b74_A935_CAFE832B05AE">
				<EAScenarioContent>
					<path>
						<step name="An Assisted person browses the uStore registration link" guid="{0EB1F11E-34BD-4532-8646-9427297BCBBE}" level="1" uses="Registration through web UI" result="" state="" trigger="1" link=""/>
						<step name="The System asks the person for related disabilities, so the content during the registration can be adjusted accordingly." guid="{CBD87E04-6640-463b-A7B3-198BADEC593C}" level="2" uses="" result="" state="" trigger="0" link=""/>
						<step name="The Assisted person fills in the realted disabilities." guid="{9AB06256-3AD8-4265-A1D4-D5E9B5B840D4}" level="3" uses="" result="" state="" trigger="1" link=""/>
						<step name="The System stores the information in the session so all future content is adjusted accordingly." guid="{2E73F027-4346-4de0-8540-1492D5BC87FB}" level="4" uses="" result="" state="" trigger="0" link=""/>
						<step name="The  Assisted person chooses her login details (username, password) considering availability and also fills in questionaire about personal details, with e-mail address being mandatory." guid="{0D3A1A7D-111F-4324-A1AA-BDBDCC90A8A5}" level="5" uses="2.4 Questionnaire for filling user's profile" result="" state="" trigger="1" link=""/>
						<step name="The System stores the collected information and sends an e-mail with a validation link to the chosen e-mail address." guid="{B97806F3-608A-497c-AA1A-B215E048309C}" level="6" uses="E-mail verification" result="" state="" trigger="0" link=""/>
						<step name="The  Assisted person receives the e-mail in his mail box and clicks the validation link." guid="{14921230-A93B-4b0f-89E0-781D6BC040AD}" level="7" uses="" result="" state="" trigger="1" link="">
							<extension level="7a" guid="{A46F0BF0-29DF-4dde-A71A-FD6802A3FF35}" join="End"/>
						</step>
						<step name="The System confirms the validation and stores the collected so far information permanently in the Assisted person 's profile. Then the user is directed to a wizard-like first-time login sequence (see next steps), which she may choose to skip as well." guid="{2D27EECA-9081-4656-AE0E-3BBC34B7C3EA}" level="8" uses="" result="" state="" trigger="0" link=""/>
						<step name="The system offers the Assisted person to join existing groups, based on his profile, group popularity or any group search criteria defined by the Assisted Person" guid="{0B9DE52F-7699-4f94-ABB3-7BA0AFDE6E04}" level="9" uses="2.1 Groups" result="" state="" trigger="0" link=""/>
						<step name="The Assisted person selects some groups and is associated with them" guid="{5C9F0196-63AB-491a-85CB-13ACD464A634}" level="10" uses="" result="" state="" trigger="1" link="">
							<extension level="10a" guid="{DF1F50CC-34EA-4824-AC66-2E99A6C15F1D}" join="{0DDA073D-D2F1-4d1e-A739-70DD926BED57}"/>
						</step>
						<step name="The System offers to the Assisted person to upload her AAL platform profile" guid="{0DDA073D-D2F1-4d1e-A739-70DD926BED57}" level="11" uses="Updating AAL Platform profile" result="" state="" trigger="0" link=""/>
						<step name="If the Assisted Person agrees, an automatic scan is performed, and then the Assisted person is allowed to edit and confirm the result. The result also contains a property if this is a home or mobile profile." guid="{C90EE7E6-4C89-416b-B197-89DFC7BC4671}" level="12" uses="" result="" state="" trigger="1" link="">
							<extension level="12a" guid="{2F1233CC-2E81-4828-85F3-E977DC43F727}" join="{CF9313A8-9250-4739-89F4-6004B400B131}"/>
						</step>
						<step name="The System stores the AAL platform profile and associates it with the Assisted Person" guid="{CF9313A8-9250-4739-89F4-6004B400B131}" level="13" uses="" result="" state="" trigger="0" link=""/>
						<step name="Finally, the  Assisted person  is asked whether she desires some additional roles in the system - developer, service provider or administrator." guid="{893709E7-ADAB-4a37-BD1B-B315C2FD5366}" level="14" uses="2.3 Kinds of roles" result="" state="" trigger="0" link="">
							<extension level="14a" guid="{735AA60C-9361-47e7-9D5C-9A2270E211F7}" join="End"/>
						</step>
						<step name="The Assisted Person selects zero or more additional roles." guid="{84BA65C7-F01B-4a1b-AD56-608837019A6E}" level="15" uses="" result="" state="" trigger="1" link="">
							<extension level="15a" guid="{CE2203F8-23B6-4f54-B8AC-389598F0180E}" join="End"/>
						</step>
						<step name="If the assisted person did select additional roles, a request e-mail is send to the administrators -s." guid="{919651C3-FC65-4755-8090-08F7CE161235}" level="16" uses="" result="" state="" trigger="0" link=""/>
					</path>
				</EAScenarioContent>
			</EAScenario>
