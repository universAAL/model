			<EAScenario name="Additional roles screen is disabled by administrators" type="Alternate" weight="0,00" subject="EAID_C72B1A9C_ABAC_4fe8_8BAC_ABCEBD8F2A2E" xmi.id="EAID_735AA60C_9361_47e7_9D5C_9A2270E211F7"/>
