				<EAScenarioContent>
					<path>
						<step name="The stored personal data is deleted after a certain amount of time has passed, and the validation link is disabled." guid="{FD27EB49-474E-4fea-B142-8DE44E136ADF}" level="1" uses="" result="" state="" trigger="0" link=""/>
					</path>
				</EAScenarioContent>
