	<XMI.extensions xmi.extender="Enterprise Architect 2.5">
		<EAModel.parameter/>
		<EAModel.scenario>
			<EAScenario name="Basic Path" type="Basic Path" weight="0,00" subject="EAID_1977BFCA_C5C7_4dc6_97D9_7FE4ABB79055" xmi.id="EAID_4264DB9E_B2A4_417a_93A3_A849A4692357">
				<EAScenarioContent>
					<path>
						<step name="The company assumes the role of a Service Provider, and appropriatley registers as such on the uStore." guid="{C6B05822-34CF-42e4-B4A5-07C93E11FB28}" level="1" uses="Registration through web UI" result="" state="" trigger="1" link=""/>
						<step name="The company creates services for all of the models of devices they wish to sell. These services have detailed description of the included hardware, possibly with pictures. These services may include software, but in most cases they would not. It is important the services to be assigned to the appropriate categories." guid="{9D9C2353-81AE-4dc8-A2C4-BA34501AC88B}" level="2" uses="Preparing a service for Upload" result="" state="" trigger="1" link=""/>
						<step name="The company uploads the services on the uStore and starts managing and selling them like any other service." guid="{170F4571-BA1A-41c3-A084-77269C21FB0E}" level="3" uses="Service upload" result="" state="" trigger="1" link=""/>
						<step name="Administrator approves the services, or gives the user's account of the company a trusted status so the services are automatically approved." guid="{2781592B-CCE2-417c-8089-3C15C7D3DAF1}" level="4" uses="Service Approval" result="" state="" trigger="1" link=""/>
						<step name="The User purchases some of the services." guid="{8BC571F1-58AC-4e2e-A34C-19FA320EF128}" level="5" uses="Purchase request, Payment" result="" state="" trigger="1" link=""/>
						<step name="The System dispatches the purchase information to the company, and creates a tracker for the hardware delivery." guid="{120E1BC2-64C3-4f8f-9B64-C53F55B46810}" level="6" uses="Ordering hardware" result="" state="" trigger="0" link=""/>
						<step name="The company fills in and maintains the information in the tracker until the actual delivery. The information could be simply a link." guid="{12AB7948-3C7B-4889-BED3-1BC6E3020CE8}" level="7" uses="Following hardware" result="" state="" trigger="1" link=""/>
					</path>
				</EAScenarioContent>
			</EAScenario>
			<EAScenario name="Basic Path" type="Basic Path" weight="1,00" subject="EAID_321FFA1F_0E30_4674_B86F_40575D8A5219" xmi.id="EAID_77C74AEC_A251_4b35_B155_44B8E65C2A24">
				<EAScenarioContent>
					<path>
						<step name="An Administrator installs the software needed for running the uStore server, and changes the password for the built-in account if he wishes to." guid="{821C8718-3166-4480-841F-5215B7D83B39}" level="1" uses="Built in Administrator account" result="" state="" trigger="1" link=""/>
						<step name="The Administrator sets up an e-mail account from which the automatic mail will be sent." guid="{BDB98796-125F-4253-B7F9-4B2C24E8B622}" level="2" uses="Built in Administrator account" result="" state="" trigger="1" link=""/>
						<step name="The Administrator specifies other properties of the server - country and local information, certificates, etc." guid="{ABAF2185-B1C5-42bf-8274-A91ABE7FCAA9}" level="3" uses="Secure session, Secure protocols for payments, Different countries" result="" state="" trigger="1" link=""/>
						<step name="The Administrator specifies whether the new server is a part of a cluster or not." guid="{265E0690-DFC6-4235-A5AC-9719AEFC837F}" level="4" uses="14.2 Scalability and loading" result="" state="" trigger="1" link=""/>
						<step name="If the new server is a part of a cluster, then the Administrator provides details about the other servers in the cluster. An administrator account for any of them is sufficient. The cluster information may be obtained automatically after providing an administrator account for some of the other servers in the cluster." guid="{ABA5B558-D5D1-465f-AD5C-73C0330986DE}" level="5" uses="14.2 Scalability and loading" result="" state="" trigger="1" link="">
							<extension level="5a" guid="{BD784E0C-DC82-4747-A4EE-FFEA54FF34AE}" join="{19AA0F03-560E-4f9a-A3E3-B91828785D6D}"/>
						</step>
						<step name="The Administrator specifies if the uStore will cooperate with other servers on the internet, and provides their type and details." guid="{19AA0F03-560E-4f9a-A3E3-B91828785D6D}" level="6" uses="Other web shop" result="" state="" trigger="1" link=""/>
					</path>
				</EAScenarioContent>
			</EAScenario>
			<EAScenario name="If the new server is not part of a cluster" type="Alternate" weight="0,00" subject="EAID_321FFA1F_0E30_4674_B86F_40575D8A5219" xmi.id="EAID_BD784E0C_DC82_4747_A4EE_FFEA54FF34AE"/>
			<EAScenario name="Administrators do not approve the update" type="Alternate" weight="1,00" subject="EAID_4B89F5AF_1B2E_4a4d_B336_33D1A42FBB7D" xmi.id="EAID_AA425557_69F1_423b_9AFB_702DA8410458">
				<EAScenarioContent>
					<path>
						<step name="Any information about the new version is deleted from uStore" guid="{6A0E6085-26B0-4620-B1F9-B308FFC24C9D}" level="1" uses="" result="" state="" trigger="0" link=""/>
					</path>
				</EAScenarioContent>
			</EAScenario>
			<EAScenario name="Basic Path" type="Basic Path" weight="2,00" subject="EAID_4B89F5AF_1B2E_4a4d_B336_33D1A42FBB7D" xmi.id="EAID_4324D990_CA06_432b_9633_4101BB6D886E">
				<EAScenarioContent>
					<path>
						<step name="A Developer selects one of the services that he has previously uploaded, or in any other way has a permission to manage." guid="{F3615762-0229-4272-8C9E-F0D129084267}" level="1" uses="Service editing and access" result="" state="" trigger="1" link=""/>
						<step name="The developer clicks the Update service link, which is only visible for services, that the developer has permissions to manage." guid="{860FAFFF-8B68-400f-9DD3-9267B464183B}" level="2" uses="Service editing and access" result="" state="" trigger="1" link=""/>
						<step name="The System displays a service update screen, containing information about the new content and the the service." guid="{CBAB13B1-06FA-4dd2-9862-F4219697B29F}" level="3" uses="" result="" state="" trigger="0" link=""/>
						<step name="The developer either confirms the upload, or changes some properties, and then confirms it." guid="{6B6F1108-51B6-488a-8540-C8D66EAB1771}" level="4" uses="" result="" state="" trigger="1" link=""/>
						<step name="The new version of the service is uploaded to uStore, but not made available yet." guid="{6E634A50-4AD5-46a5-A6F8-C58EB1C11F59}" level="5" uses="Update Service upload, Software from eclipse plug in" result="" state="" trigger="0" link=""/>
						<step name="If the developer is not trusted, then a confirmation notification is sent to the administrators" guid="{FC41BCBB-EFB6-4085-98EA-743785F6982F}" level="6" uses="" result="" state="" trigger="0" link="">
							<extension level="6a" guid="{299225C1-B0E6-40d6-B371-0E54F90A2249}" join="{D2146B1F-076E-477c-A6FA-9927019E3CCD}"/>
